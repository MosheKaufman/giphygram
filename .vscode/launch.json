{
  "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch push.js",
            "type": "node",
            "request": "launch",
            "program": "${workspaceRoot}/push.js",
            "restart": true,
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen"
        },
        {
            "name": "Chrome",
            "type": "chrome",
            "request": "launch",
            "url": "http://localhost:8887",
            "webRoot": "${workspaceRoot}"
        }
    ]
}